izer;","    pattern::Regex = r\"\\w\\w+\\b\", max_length::Union{Nothing, Int}=nothing, transform=simplify","    )","    words = map(m->string(m.match), eachmatch(pattern, transform(document)))","    tokens = tokenizer(words)","    if !isnothing(max_length)","        if length(tokens) > max_length","            tokens = tokens[1:max_length]","        end","    end","    tokens","end"],"stylingDirectives":[[{"start":0,"end":5,"cssClass":"pl-k"}],[{"start":0,"end":5,"cssClass":"pl-k"}],[],[{"start":0,"end":13,"cssClass":"pl-c"},{"start":0,"end":1,"cssClass":"pl-c"}],[],[{"start":0,"end":8,"cssClass":"pl-k"},{"start":9,"end":26,"cssClass":"pl-en"},{"start":33,"end":35,"cssClass":"pl-k"},{"start":35,"end":67,"cssClass":"pl-c1"}],[{"start":26,"end":28,"cssClass":"pl-k"},{"start":28,"end":31,"cssClass":"pl-c1"},{"start":31,"end":32,"cssClass":"pl-k"},{"start":32,"end":34,"cssClass":"pl-c1"},{"start":43,"end":45,"cssClass":"pl-k"},{"start":45,"end":50,"cssClass":"pl-c1"},{"start":50,"end":51,"cssClass":"pl-k"},{"start":51,"end":61,"cssClass":"pl-sr"},{"start":51,"end":53,"cssClass":"pl-pds"},{"start":53,"end":57,"cssClass":"pl-cce"},{"start":58,"end":60,"cssClass":"pl-cce"},{"start":60,"end":61,"cssClass":"pl-pds"},{"start":72,"end":73,"cssClass":"pl-k"}],[{"start":25,"end":26,"cssClass":"pl-k"},{"start":27,"end":38,"cssClass":"pl-c1"},{"start":38,"end":51,"cssClass":"pl-c1"},{"start":52,"end":53,"cssClass":"pl-c1"}],[{"start":4,"end":7,"cssClass":"pl-k"},{"start":17,"end":19,"cssClass":"pl-k"}],[{"start":14,"end":15,"cssClass":"pl-k"},{"start":16,"end":19,"cssClass":"pl-c1"},{"start":19,"end":27,"cssClass":"pl-c1"}],[{"start":8,"end":11,"cssClass":"pl-k"},{"start":14,"end":16,"cssClass":"pl-k"},{"start":17,"end":26,"cssClass":"pl-c1"},{"start":36,"end":45,"cssClass":"pl-c1"}],[{"start":17,"end":18,"cssClass":"pl-k"},{"start":20,"end":21,"cssClass":"pl-k"}],[{"start":12,"end":14,"cssClass":"pl-k"},{"start":15,"end":16,"cssClass":"pl-k"},{"start":22,"end":24,"cssClass":"pl-k